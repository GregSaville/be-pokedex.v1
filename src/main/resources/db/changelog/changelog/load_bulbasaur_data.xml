<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd
		http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
		http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.9.xsd">
    <changeSet id="02" author="gsaville">
        <insert tableName="pokemon">
            <column name="id" value="1"/>
            <column name="name" value="Bulbasaur"/>
            <column name="height" value="7.00"/>
            <column name="weight" value="69.00"/>
            <column name="genus" value="Seed Pokémon"/>
            <column name="description" value="Bulbasaur can be seen napping in bright sunlight. There is a seed on its back. By soaking up the sun’s rays"/>
        </insert>
        <insert tableName="abilities">
            <column name="pokeId" value="1"/>
            <column name="ability1" value="chlorophyll"/>
            <column name="ability2" value="overgrow"/>
            <column name="ability3" value="null"/>
        </insert>
        <insert tableName="egg_group">
            <column name="pokeId" value="1"/>
            <column name="group1" value="plant"/>
            <column name="group2" value="monster"/>
        </insert>
        <insert tableName="typing">
            <column name="pokeId" value="1"/>
            <column name="type1" value="grass"/>
            <column name="type2" value="null"/>
        </insert>
        <insert tableName="stats">
            <column name="pokeId" value="1"/>
            <column name="hp" value="45"/>
            <column name="speed" value="45"/>
            <column name="attack" value="49"/>
            <column name="defense" value="49"/>
            <column name="special_attack" value="65"/>
            <column name="special_defense" value="65"/>
        </insert>
    </changeSet>
</databaseChangeLog>

