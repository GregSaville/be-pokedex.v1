<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd
		http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
		http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.9.xsd">
    <changeSet id="02" author="gsaville">
        <insert tableName="pokemon">
            <column name="id" value="4"/>
            <column name="name" value="Charmander"/>
            <column name="height" value="6.00"/>
            <column name="weight" value="85.00"/>
            <column name="genus" value="Lizard Pokémon"/>
            <column name="description" value="The flame that burns at the tip of its tail is an indication of its emotions. The flame wavers when Charmander is enjoying itself. If the Pokémon becomes enraged, the flame burns fiercely."/>
        </insert>
        <insert tableName="abilities">
            <column name="pokeId" value="4"/>
            <column name="ability1" value="solar-power"/>
            <column name="ability2" value="blaze"/>
            <column name="ability3" value="null"/>
        </insert>
        <insert tableName="egg_group">
            <column name="pokeId" value="4"/>
            <column name="group1" value="dragon"/>
            <column name="group2" value="monster"/>
        </insert>
        <insert tableName="typing">
            <column name="pokeId" value="4"/>
            <column name="type1" value="fire"/>
            <column name="type2" value="null"/>
        </insert>
        <insert tableName="stats">
            <column name="pokeId" value="4"/>
            <column name="hp" value="39"/>
            <column name="speed" value="65"/>
            <column name="attack" value="52"/>
            <column name="defense" value="43"/>
            <column name="special_attack" value="60"/>
            <column name="special_defense" value="50"/>
        </insert>
    </changeSet>
</databaseChangeLog>

